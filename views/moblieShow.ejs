<style>
    html, body {
        margin: 0;
        padding: 0;
        width:100%;
        height:100%;
    }

    ul, ol {
        -webkit-margin-before: 0;
        -webkit-margin-after: 0;
        -webkit-margin-start: 0;
        -webkit-margin-end: 0;
        padding:0;
        margin:0;
    }

    body * {
        -webkit-text-size-adjust:none;
        /*-webkit-user-select: none;*/
        -webkit-tap-highlight-color: rgba(0,0,0,0);

    }


    .mobile-version-silder li{
        width:320px;


    }
    .mobile-version-silder li img{
        max-width:320px;
    }
</style>

<div class="mobile-version-wrap">
    <div class="mobile-version-silder" id="J-versionSilder">
        <ul>
            <% pictures.forEach( function ( picture ){ %>
            <li>
                <img src="/images/<%= picture.projectDir%>/<%= picture.versionDir%>/<%= picture.path%>" alt="">
             </li>
            <%})%>
        </ul>
    </div>
</div>




<script type="text/javascript" src="/javascripts/swipe.js"></script>
<script type="text/javascript">

        var t = new Swipe(document.getElementById('J-versionSilder'),{
            fixWidth:"320"
        });
        $(".J-prev").on("click",function(e){
            e.preventDefault();
            t.prev();
        });
        $(".J-next").on("click",function(e){
            e.preventDefault();
            t.next();
        });
</script>