<!DOCTYPE html>
<html>
<head>
    <title>项目</title>
    <link rel="stylesheet" href="/stylesheets/style.css"/>
    <link rel="stylesheet" href="/stylesheets/flow.css"/>
    <script type="text/javascript" src="http://g.tbcdn.cn/kissy/k/1.3.0/kissy-min.js"></script>

</head>
<body>
<div class="header-bar">
    <h1 class="clearfix"><a href="/" id="logo">手机keynote</a></h1>

</div>



<div class="main clearfix">


    
    <div class="content">
        <div class="c">

            <%- body%>
        </div>
    </div>

    <div class="sidebar" id="J-sidebar">
        <div class="user-info clearfix">
            <div class="avatar">
                <span class="bg"></span>
                <img src="http://api.yunpan.alibaba.com/getPhoto?userId=<%= userInfo.uid%>&zoomSize=50"/>
            </div>
            <div class="desc">
                <span class="username"><%= userInfo.nick%></span>
            </div>
        </div>
        <div class="add-btn-wrap">
            <a href="#" class="add-btn" id="J-addDirBtn"></a>
        </div>
        <div class="sidebar-content">
            <ul id="J-sidebarList" class="side-list">
                <% projects.forEach( function ( project ){ %>
                <li>
                    <a href="/file/<%= project.id %>"> <%= project.name%></a>
                </li>
                <%})%>
            </ul>
        </div>
    </div>

</div>


<script type="text/javascript" src="/javascripts/sidebar.js"></script>
<script>
    KISSY.use('sidebar',function(S,Sidebar){
        new Sidebar();
    });
</script>


</body>
</html>